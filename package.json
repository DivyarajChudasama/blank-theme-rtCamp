{
	"name": "blank-theme",
	"version": "2.0.0",
	"description": "Blank Theme Packages",
	"author": "rtCamp",
	"license": "MIT",
	"private": true,
	"scripts": {
		"clean": "rm -rf build/*",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"init": "npm install; ./bin/rename.js; npm run update-deps; npm run pot; rm -rf .git .github ./bin/ languages/blank-theme.pot;",
		"lint-css": "stylelint sass/**/*.scss --syntax scss",
		"lint-js": "eslint js/",
		"pot": "wp-pot --src '**/*.php' --dest-file 'languages/blank-theme.pot' --domain 'blank-theme' --package 'Blank Theme' ",
		"precommit": "npm run lint-css && npm run lint-js",
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"update-deps": "rm -rf node_modules && rm package-lock.json && npm install"
	},
	"dependencies": {
		"prompt-sync": "^4.1.6",
		"replace-in-file": "^3.4.2"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"autoprefixer": "^9.1.3",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.2",
		"babel-preset-env": "^1.7.0",
		"clean-webpack-plugin": "^0.1.19",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"css-mqpacker": "^7.0.0",
		"cssnano": "^4.1.0",
		"eslint": "^5.5.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-loader": "^2.1.0",
		"foundation-sites": "^6.5.0-rc.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"fs-extra": "^7.0.0",
		"jquery": "^3.3.1",
		"mini-css-extract-plugin": "^0.4.2",
		"node-sass": "^4.9.3",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"postcss-loader": "^3.0.0",
		"postcss-pxtorem": "^4.0.1",
		"postcss-scss": "^2.0.0",
		"sass-loader": "^7.1.0",
		"sass-mq": "^5.0.0",
		"slick-carousel": "^1.8.1",
		"style-loader": "^0.23.0",
		"stylelint": "^9.5.0",
		"stylelint-config-wordpress": "^13.1.0",
		"stylelint-webpack-plugin": "^0.10.5",
		"uglifyjs-webpack-plugin": "^2.0.1",
		"webpack": "^4.17.1",
		"webpack-assets-manifest": "^3.0.2",
		"webpack-cli": "^3.1.0",
		"wp-pot-cli": "^1.2.1"
	},
	"browserslist": [
		"defaults"
	]
}
